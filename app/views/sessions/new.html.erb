<h1>Sign In</h1>

<form action="<%= session_url %>" method="POST">
  <%= auth_token %>

  <label>Username
  <input type="text" name="user[username]">
  </label>

  <label for="password_form">Password</label>
  <input type="text" name="user[password]" id="password_form">

  <input type="submit" value="Log In">


</form>
